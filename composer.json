{
    "name": "awssat/numbered-string-order",
    "type": "library",
    "description": "Sort a string array based on the included numbers naturally.",
    "keywords": [
        "sort", "php", "array"
    ],
    "homepage": "https://github.com/Awssat/numberedStringOrder",
    "license": "MIT",
    "authors": [
        {
            "name": "Awssat",
            "email": "hello@awssat.com"
        }
    ],
    "require": {
        "php" : "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Awssat\\numberedStringOrder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awssat\\numberedStringOrder\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
